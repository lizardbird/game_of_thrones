<h2>Edit <%= @character.name %> from House <%= @house.name %></h2>

<%= form_for [@house, @character] do |f| %>
  <%= f.label :First_name %>
  <%= f.text_field :name %>
  <%= f.label :Img_URL %>
  <%= f.text_field :img_url %>
  <%= f.label :"Alive? (true or false)" %>
  <%= f.text_field :is_alive %>

  <%= f.submit class: 'submit'%>
<% end %>




<%= form_for [@house, @character], method: :delete do |f| %>
  <%= f.submit "Delete!", class: "delete" %>
<% end %>

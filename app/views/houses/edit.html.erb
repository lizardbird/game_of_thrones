<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>
  <%= f.label :seat %>
  <%= f.text_field :location %>
  <%= f.label :region %>
<%= f.text_field :region %>
  <%= f.submit class: 'submit' %>
<% end %>




<%= form_for @house, method: :delete do |f| %>
  <%= f.submit "Delete!", class: "delete" %>
<% end %>
